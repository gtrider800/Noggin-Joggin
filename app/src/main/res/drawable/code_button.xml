<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:top="0dp" android:left="0dp" android:right="0dp" android:bottom="0dp">
        <shape android:shape="rectangle">
            <size android:width="50dp" android:height="50dp" />
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>
    <item android:top="30dp" android:left="0dp" android:right="0dp" android:bottom="0dp">
        <shape android:shape="oval">
            <solid android:color="@color/code_default"/>
            <stroke
                    android:width="1dp"
                    android:color="@color/black"/>
        </shape>
    </item>
    <item android:top="7dp" android:left="0dp" android:right="0dp" android:bottom="7dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/code_default"/>
            <corners android:radius="2dp"/> <!-- Adjust radius for rounded corners -->
            <stroke
                    android:width="1dp"
                    android:color="@color/black"/>
        </shape>
    </item>
    <item android:top="8dp" android:left="1.74dp" android:right="1.72dp" android:bottom="6.7dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/code_default"/>
            <corners android:radius="2dp"/>
        </shape>
    </item>
    <item android:top="0dp" android:left="0dp" android:right="0dp" android:bottom="30dp">
        <shape android:shape="oval">
            <solid android:color="@color/code_default"/>
            <stroke
                    android:width="1dp"
                    android:color="@color/black"/>
        </shape>
    </item>
</layer-list>